<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://static.spryker.com http://static.spryker.com/transfer-01.xsd" >

    <transfer name="Lottery">
        <property name="firstName" type="string" />
        <property name="lastName" type="string" />
        <property name="email" type="string" />
    </transfer>

    <transfer name="LotteryApiResponse">
        <property name="status" type="string" />
        <property name="description" type="string" />
    </transfer>

    <transfer name="Quote">
        <property name="lottery" type="Lottery" />
    </transfer>

</transfers>
